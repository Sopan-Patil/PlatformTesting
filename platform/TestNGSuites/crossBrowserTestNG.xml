<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CrossBrowserSuite" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="testscripts.Listeners" />
	</listeners>
	<test name="ChromeTest">
		<parameter name="localBrowser" value="chrome" />
		<classes>
			 <class name="testscripts.LoginVerification"/>
		</classes>
	</test>
	<test name="EdgeTest">
		<parameter name="localBrowser" value="edge" />
		<classes>
			 <class name="testscripts.LoginVerification"/>
		</classes>
	</test>
</suite> <!-- Suite -->
